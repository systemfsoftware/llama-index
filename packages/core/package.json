{
  "name": "@systemfsoftware/llama-index_core",
  "version": "0.0.1",
  "type": "module",
  "author": "Ryan Lee <ryanleecode@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/systemfsoftware/llama-index.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@systemfsoftware/source": "./src/mod.ts",
        "types": "./dist/esm/mod.d.ts",
        "default": "./dist/esm/mod.js"
      }
    },
    "./schema": {
      "import": {
        "@systemfsoftware/source": "./src/schema.ts",
        "types": "./dist/esm/schema.d.ts",
        "default": "./dist/esm/schema.js"
      }
    },
    "./embeddings": {
      "import": {
        "@systemfsoftware/source": "./src/embeddings.ts",
        "types": "./dist/esm/embeddings.d.ts",
        "default": "./dist/esm/embeddings.js"
      }
    }
  },
  "module": "./dist/esm/mod.js",
  "files": [
    "dist"
  ],
  "tshy": {
    "dialects": [
      "esm"
    ],
    "project": "./tsconfig.build.json",
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/mod.ts",
      "./schema": "./src/schema.ts",
      "./embeddings": "./src/embeddings.ts"
    },
    "sourceDialects": [
      "@systemfsoftware/source"
    ]
  },
  "scripts": {
    "prepare": "turbo build",
    "clean": "rimraf dist .tshy .tshy-build",
    "build": "pnpm run clean && tshy"
  },
  "devDependencies": {
    "@llamaindex/core": "^0.1.10",
    "effect": "^3.7.2"
  },
  "peerDependencies": {
    "llamaindex": "^0.5",
    "@llamaindex/core": "^0.1"
  }
}
