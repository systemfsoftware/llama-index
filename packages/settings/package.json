{
  "name": "@systemfsoftware/llama-index_settings",
  "version": "0.0.1",
  "type": "module",
  "author": "Ryan Lee <ryanleecode@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/systemfsoftware/llama-index.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@systemfsoftware/source": "./src/mod.ts",
        "types": "./dist/esm/mod.d.ts",
        "default": "./dist/esm/mod.js"
      }
    }
  },
  "module": "./dist/esm/mod.js",
  "files": [
    "dist"
  ],
  "tshy": {
    "dialects": [
      "esm"
    ],
    "project": "./tsconfig.build.json",
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/mod.ts"
    },
    "sourceDialects": [
      "@systemfsoftware/source"
    ]
  },
  "scripts": {
    "prepare": "turbo build",
    "clean": "rimraf dist .tshy .tshy-build",
    "build": "pnpm run clean && tshy"
  },
  "dependencies": {},
  "devDependencies": {
    "effect": "^3.7.2",
    "llamaindex": "^0.5.21"
  },
  "peerDependencies": {
    "llamaindex": "^0.5",
    "effect": "^3.7"
  },
  "peerDependenciesMeta": {
    "llamaindex": {
      "optional": false
    },
    "effect": {
      "optional": false
    }
  }
}
