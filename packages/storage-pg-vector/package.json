{
  "name": "@systemfsoftware/llama-index_storage-pg-vector",
  "version": "0.0.4",
  "type": "module",
  "author": "Ryan Lee <ryanleecode@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/systemfsoftware/llama-index.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@systemfsoftware/source": "./src/mod.ts",
        "types": "./dist/esm/mod.d.ts",
        "default": "./dist/esm/mod.js"
      }
    }
  },
  "module": "./dist/esm/mod.js",
  "keywords": [
    "effect",
    "llamaindex",
    "ai",
    "vector-store",
    "typescript",
    "pg"
  ],
  "files": [
    "dist"
  ],
  "tshy": {
    "dialects": [
      "esm"
    ],
    "project": "./tsconfig.build.json",
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/mod.ts"
    },
    "sourceDialects": [
      "@systemfsoftware/source"
    ]
  },
  "scripts": {
    "prepare": "turbo build",
    "clean": "rimraf dist .tshy .tshy-build",
    "build": "pnpm run clean && tshy"
  },
  "dependencies": {
    "kysely": "^0.27.4",
    "pgvector": "^0.2.0"
  },
  "devDependencies": {
    "pg": "^8.12.0",
    "effect": "^3.7.2",
    "@systemfsoftware/llama-index_storage": "workspace:^",
    "@systemfsoftware/llama-index_settings": "workspace:^",
    "@types/pg": "^8.11.8"
  },
  "peerDependencies": {
    "@systemfsoftware/llama-index_settings": "^0.0.2",
    "@systemfsoftware/llama-index_storage": "^0.0.2",
    "pg": "^8.1",
    "effect": "^3.7"
  },
  "peerDependenciesMeta": {
    "@systemfsoftware/llama-index_settings": {
      "optional": false
    },
    "@systemfsoftware/llama-index_storage": {
      "optional": false
    },
    "pg": {
      "optional": false
    },
    "effect": {
      "optional": false
    }
  }
}
